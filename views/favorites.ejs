<%- include('partials/header.ejs') %>
<style>
    body{
        background-repeat: repeat;
        background-size: auto;
    }
</style>
   <a class="link" href="/"> Back to Search </a>
    <main>
   <h1 id="favHeader">Favorite Pics</h1>
    
   <% 
       //displaying five keywords per line
      rows.forEach(function(row, i){ %>
       
        <a class="keywordLink" href="#"><%= row["keyword"] %></a>
    
        <%- ((i+1) % 6 == 0)?'<br><br>':'' %>
    
    <% });%>
    </main>
    
    <div id="favorites"></div>
    
    <input type="hidden" id="keywordSelected">

